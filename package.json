{
  "name": "graph-text-adventure",
  "version": "0.0.2",
  "description": "Graph DB backed text adventure",
  "main": "app.js",
  "scripts": {
    "start": "npm run all",
    "all": "concurrently --kill-others \"node npc/server.js\" \"node god/server.js\" \"node player/server.js\" \"node server.js\"",
    "all-mon": "concurrently --kill-others \"nodemon npc/server.js\" \"nodemon god/server.js\" \"nodemon player/server.js\" \"nodemon server.js\"",
    "app": "node app.js",
    "app-mon": "nodemon app.js",
    "dev": "npm run mon & npm run app-mon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRealCodeBeard/GraphTextAdventure.git"
  },
  "author": "Phil Harvey",
  "contributors": [
    { "name": "Ben Coleman" }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheRealCodeBeard/GraphTextAdventure/issues"
  },
  "homepage": "https://github.com/TheRealCodeBeard/GraphTextAdventure#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "god-api": "file:god",
    "gremlin": "2.7.0",
    "npc-api": "file:npc",
    "player-api": "file:player",
    "readline": "^1.3.0",
    "shared": "file:shared"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}

{
  "name": "graph-text-adventure",
  "version": "0.0.2",
  "description": "Graph DB backed text adventure",
  "main": "app.js",
  "scripts": {
    "start": "npm run all",
    "all": "concurrently --kill-others \"node npc/server.js\" \"node god/server.js\" \"node player/server.js\" \"node world/server.js\"",
    "all-mon": "concurrently --kill-others \"nodemon npc/server.js\" \"nodemon god/server.js\" \"nodemon player/server.js\" \"nodemon world/server.js\"",
    "console": "node clients/console/app.js",
    "console-mon": "nodemon clients/console/app.js",
    "dev": "npm run mon & npm run app-mon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRealCodeBeard/GraphTextAdventure.git"
  },
  "author": "Phil Harvey",
  "contributors": [
    {
      "name": "Ben Coleman"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheRealCodeBeard/GraphTextAdventure/issues"
  },
  "homepage": "https://github.com/TheRealCodeBeard/GraphTextAdventure#readme",
  "dependencies": {
    "shared": "file:shared",
    "client-console": "file:clients/console",
    "god-api": "file:god",
    "npc-api": "file:npc",
    "player-api": "file:player",
    "world-api": "file:world"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
